class Main{
    public static void main(String[]args){
        int num=80150;
        int rem=0;
        int i=1;
        int result=0;
        while(num>0){
            rem=num%10;
            if(rem==0){
                rem=1;
            }
            else if(rem==1){
                rem=0;
            }
            result+=rem*i;
            num=num/10;
            i*=10;

        }
        System.out.println(result);
    }
}
